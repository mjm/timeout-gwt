        <dependency org="com.google.gwt" name="gwt-user" rev="2.0.0"/>
        <dependency org="com.google.gwt" name="gwt-dev" rev="2.0.0"/>
        <dependency org="org.apache.ibatis" name="ibatis-sqlmap" rev="3.0-beta-4"/>
        <dependency org="org.springframework" name="org.springframework.core" rev="3.0.0.RELEASE"
                    conf="default->log4j"/>
        <dependency org="org.springframework" name="org.springframework.beans" rev="3.0.0.RELEASE"
                    conf="default->bytecode-proxy"/>
        <dependency org="org.springframework" name="org.springframework.orm" rev="3.0.0.RELEASE"
                    conf="default->web"/>
        <dependency org="org.springframework" name="org.springframework.web" rev="3.0.0.RELEASE"
