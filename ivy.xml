        <dependency org="com.mchange.c3p0" name="com.springsource.com.mchange.v2.c3p0" rev="0.9.1.2"/>
        <dependency org="net.customware.gwt.dispatch" name="gwt-dispatch" rev="1.1.0-SNAPSHOT"/>
        <dependency org="net.customware.gwt.presenter" name="gwt-presenter" rev="1.1.0-replace-SNAPSHOT"/>
        <dependency org="com.google.gwt" name="gin" rev="1.0"/>
        <dependency org="com.google.inject" name="guice" rev="2.0"/>
        <dependency org="org.mockito" name="com.springsource.org.mockito" rev="1.8.0"/>
    </dependencies>
